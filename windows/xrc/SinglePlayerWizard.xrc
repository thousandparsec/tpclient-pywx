  <object class="wxPanel" name="OpponentPage" subclass="wxWizardPage">
    <object class="wxFlexGridSizer">
      <object class="sizeritem">
        <object class="wxStaticText" name="wxID_STATIC">
          <size>400,-1</size>
          <label>Opponents</label>
          <font>
            <size>14</size>
            <weight>bold</weight>
            <family>swiss</family>
            <face>Sans</face>
          </font>
          <style>wxALIGN_CENTRE</style>
        </object>
        <option>1</option>
        <flag>wxEXPAND|wxGROW</flag>
      </object>
      <object class="sizeritem">
        <object class="wxStaticLine">
          <size>300,1</size>
        </object>
      </object>
      <object class="sizeritem">
        <object class="wxStaticText" name="AIListLabel">
          <size>400,-1</size>
          <label>This page lets you add AI opponents to your game.</label>
          <wrap>400</wrap>
          <style>wxALIGN_LEFT</style>
        </object>
        <option>1</option>
        <flag>wxEXPAND|wxGROW</flag>
      </object>
      <object class="sizeritem">
        <object class="wxFlexGridSizer">
          <object class="sizeritem">
            <object class="wxListCtrl" name="Opponents">
              <size>400,-1</size>
              <style>wxLC_REPORT</style>
            </object>
            <option>1</option>
            <flag>wxEXPAND|wxGROW</flag>
            <ratio>1</ratio>
          </object>
          <cols>1</cols>
          <rows>1</rows>
          <growablecols>0</growablecols>
          <growablerows>0</growablerows>
        </object>
        <option>1</option>
        <flag>wxEXPAND|wxGROW</flag>
      </object>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <object class="sizeritem">
                <object class="wxStaticText" name="AIClientLabel">
                  <label>Type</label>
                  <style>wxALIGN_RIGHT</style>
                </object>
                <option>1</option>
                <flag>wxALIGN_RIGHT|wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxComboBox" name="AIClient">
                  <tooltip>The AI client to use.</tooltip>
                  <style>wxCB_READONLY</style>
                </object>
                <flag>wxEXPAND</flag>
              </object>
              <object class="spacer"/>
              <object class="sizeritem">
                <object class="wxStaticText" name="AIClientDesc">
                  <label>The AI description will go here.</label>
                  <style>wxALIGN_LEFT</style>
                </object>
                <option>1</option>
                <flag>wxALL|wxEXPAND</flag>
                <border>4</border>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText" name="AIUserLabel">
                  <label>Name</label>
                  <style>wxALIGN_RIGHT</style>
                </object>
                <option>1</option>
                <flag>wxALIGN_RIGHT|wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="AIUser"/>
                <option>1</option>
                <flag>wxEXPAND|wxGROW</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText" name="AIOptionsLabel">
                  <label>Options</label>
                  <style>wxALIGN_RIGHT|wxST_NO_AUTORESIZE</style>
                </object>
                <option>1</option>
                <flag>wxALIGN_RIGHT|wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxFlexGridSizer">
                  <object class="sizeritem">
                    <object class="wxPanel" name="SizerRef"/>
                    <flag>wxEXPAND|wxGROW</flag>
                  </object>
                  <cols>2</cols>
                  <vgap>5</vgap>
                  <hgap>5</hgap>
                  <growablecols>1</growablecols>
                </object>
                <option>1</option>
                <flag>wxEXPAND</flag>
              </object>
              <object class="spacer"/>
              <object class="sizeritem">
                <object class="wxFlexGridSizer">
                  <object class="spacer"/>
                  <object class="sizeritem">
                    <object class="wxButton" name="wxID_NEW">
                      <label>&amp;New</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxButton" name="wxID_SAVE">
                      <label>&amp;Save</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxButton" name="wxID_DELETE">
                      <label>&amp;Delete</label>
                    </object>
                  </object>
                  <cols>5</cols>
                  <rows>2</rows>
                  <growablecols>0</growablecols>
                  <growablerows>1,3</growablerows>
                </object>
                <option>1</option>
                <flag>wxEXPAND|wxGROW</flag>
              </object>
              <cols>2</cols>
              <vgap>1</vgap>
              <hgap>5</hgap>
              <growablecols>1</growablecols>
            </object>
            <option>1</option>
            <flag>wxEXPAND</flag>
          </object>
          <label>New Opponent</label>
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <object class="wxStaticText" name="BoxRef">
              <size>1,1</size>
            </object>
          </object>
        </object>
        <option>1</option>
        <flag>wxEXPAND|wxGROW</flag>
      </object>
      <cols>1</cols>
      <vgap>5</vgap>
      <growablecols>0</growablecols>
      <growablerows>2,3</growablerows>
    </object>
