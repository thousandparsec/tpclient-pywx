<?xml version="1.0" encoding="UTF-8"?>
<resource version="2.3.0.1" xmlns="http://www.wxwidgets.org/wxxrc">
  <object class="wxWizard" name="SinglePlayerWizard">
    <title>TP: Single Player Game</title>
    <bitmap>graphics/sidebar.bmp</bitmap>
  </object>
  <object class="wxPanel" name="StartPage" subclass="wxWizardPage">
    <pos>0,0</pos>
    <size>400,299</size>
    <object class="wxFlexGridSizer">
      <object class="sizeritem">
        <object class="wxStaticText" name="wxID_STATIC">
          <label>Single Player Game</label>
          <font>
            <size>14</size>
            <weight>bold</weight>
            <family>swiss</family>
            <face>Sans</face>
          </font>
          <style>wxALIGN_CENTRE</style>
        </object>
      </object>
      <object class="sizeritem">
        <object class="wxStaticLine">
          <size>300,1</size>
        </object>
      </object>
      <object class="sizeritem">
        <object class="wxStaticText" name="PageDesc">
          <size>400,-1</size>
          <label>This wizard sets up a single player Thousand Parsec game using the servers, rulesets, and AI clients installed locally on your system.</label>
          <wrap>400</wrap>
          <style>wxNO_BORDER|wxALIGN_LEFT|wxST_NO_AUTORESIZE</style>
        </object>
        <flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
      </object>
      <object class="sizeritem">
        <object class="wxStaticText" name="ProceedDesc">
          <size>400,-1</size>
          <wrap>400</wrap>
          <style>wxNO_BORDER|wxALIGN_LEFT|wxST_NO_AUTORESIZE</style>
        </object>
        <flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
      </object>
      <cols>1</cols>
      <object class="sizeritem">
        <object class="wxHyperlinkCtrl" name="DownloadLink">
          <label>Download Servers</label>
          <style>wxNO_BORDER|wxHL_ALIGN_LEFT</style>
        </object>
        <flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
      </object>
      <vgap>5</vgap>
      <growablecols>1</growablecols>
      <growablerows/>
    </object>
  </object>
  <object class="wxPanel" name="RulesetPage" subclass="wxWizardPage">
    <object class="wxFlexGridSizer">
      <object class="sizeritem">
        <object class="wxStaticText" name="wxID_STATIC">
          <size>400,-1</size>
          <label>Select Ruleset</label>
          <wrap>400</wrap>
          <font>
            <size>14</size>
            <weight>bold</weight>
            <family>swiss</family>
            <face>Sans</face>
          </font>
          <style>wxALIGN_CENTRE</style>
        </object>
      </object>
      <object class="sizeritem">
        <object class="wxStaticLine">
          <size>300,1</size>
        </object>
      </object>
      <object class="sizeritem">
        <object class="wxStaticText" name="wxID_STATIC">
          <size>400,-1</size>
          <label>Choose a ruleset for your game:</label>
          <wrap>400</wrap>
          <style>wxALIGN_LEFT</style>
        </object>
      </object>
      <object class="sizeritem">
        <object class="wxComboBox" name="Ruleset">
          <tooltip>The ruleset to use.</tooltip>
          <style>wxCB_READONLY</style>
        </object>
        <flag>wxEXPAND</flag>
      </object>
      <object class="sizeritem">
        <object class="wxStaticText" name="RulesetDesc">
          <size>400,-1</size>
          <wrap>400</wrap>
          <style>wxNO_BORDER|wxALIGN_LEFT|wxST_NO_AUTORESIZE</style>
        </object>
        <flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
      </object>
      <object class="sizeritem">
        <object class="wxStaticText" name="DownloadDesc">
          <size>400,-1</size>
          <wrap>400</wrap>
          <style>wxNO_BORDER|wxALIGN_LEFT|wxST_NO_AUTORESIZE</style>
        </object>
        <flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
      </object>
      <object class="sizeritem">
        <object class="wxHyperlinkCtrl" name="DownloadLink">
          <label>Download Additional Servers/Rulesets</label>
          <style>wxNO_BORDER|wxHL_ALIGN_LEFT</style>
        </object>
        <flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
      </object>
      <cols>1</cols>
      <vgap>5</vgap>
      <growablecols>1</growablecols>
      <growablerows>4,5</growablerows>
    </object>
    <pos>0,0</pos>
    <size>400,299</size>
  </object>
